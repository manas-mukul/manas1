#include<stdio.h>
int getinput(int a)
{
  scanf("%d",&a);
  return a;
  }
  int add(int b, int c)
  { int d;
   d=b+c;
        return d;
        }
        void output(int e)
{
        printf("sum=%d",e);
}
main()
{
int p,q,z,f,g;
printf("enter two numbers\n");
f=getinput(p);
g=getinput(q);
z=add(f,g);
output(z);
                getch();
